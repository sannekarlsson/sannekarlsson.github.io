{
  "name": "sannekarlsson",
  "version": "1.0.0",
  "private": true,
  "homepage": "https://sannekarlsson.github.io",
  "author": "Sanne Karlsson",
  "license": "MIT",
  "scripts": {
    "build": "parcel build src/index.pug -d build --public-url .",
    "deploy": "gh-pages -b master -d build",
    "postbuild": "cp README.md build",
    "prebuild": "rm -rf build",
    "predeploy": "npm run build",
    "prestart": "rm -rf dist",
    "start": "parcel src/index.pug"
  },
  "dependencies": {
    "gh-pages": "^2.0.1"
  },
  "devDependencies": {
    "autoprefixer": "^9.5.1",
    "cssnano": "^4.1.10",
    "pug": "^2.0.3"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "postcss": {
    "plugins": {
      "autoprefixer": true
    }
  }
}
