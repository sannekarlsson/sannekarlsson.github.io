{
  "name": "sannekarlsson",
  "version": "1.0.0",
  "private": true,
  "homepage": "https://sannekarlsson.github.io",
  "author": "Sanne Karlsson",
  "license": "MIT",
  "scripts": {
    "build": "parcel build src/index.pug -d build --public-url .",
    "deploy": "gh-pages -b master -d build",
    "postbuild": "cp README.md build",
    "posttest:local:ie": "kill $(lsof -ti :8083) && echo Port closed",
    "prebuild": "rm -rf build",
    "predeploy": "npm run build",
    "prestart": "rm -rf dist",
    "pretest": "npm run build",
    "start": "parcel src/index.pug --public-url .",
    "test": "run-p test:local:*",
    "test:local:ie": "node tests/local.runner.js -e local.ie -g local",
    "test:local:server": "live-server --port=8083 --no-browser --watch=build/index.html",
    "local-server": "live-server --port=8083 --open=build --watch=build/index.html"
  },
  "dependencies": {
    "dotenv": "^8.2.0",
    "gh-pages": "^2.2.0"
  },
  "devDependencies": {
    "autoprefixer": "^9.8.6",
    "browserstack-local": "^1.4.5",
    "cssnano": "^4.1.10",
    "jstransformer-scss": "^1.0.0",
    "live-server": "^1.2.1",
    "nightwatch": "^1.3.7",
    "npm-run-all": "^4.1.5",
    "pug": "^2.0.4",
    "sass": "^1.26.10"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "last 2 versions",
    "not ie <= 8",
    "iOS > 10"
  ],
  "postcss": {
    "plugins": {
      "autoprefixer": true
    }
  },
  "sass": {
    "style": "compressed"
  }
}